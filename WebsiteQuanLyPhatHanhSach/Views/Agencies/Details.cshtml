@model WebsiteQuanLyPhatHanhSach.Models.Agency

@{
    Layout = null;
}

<div class="modal-content">
    <div class="modal-header">
        <h4 class="modal-title"><label id="title">Details</label></h4>
    </div>
    <div class="modal-body">
        <div>
            <dl class="dl-horizontal">
                <dt>Mã đại lý</dt>
                <dd>@Html.DisplayFor(model => model.AgencyID)</dd>

                <dt>Username</dt>
                <dd>@Html.DisplayFor(model => model.AgencyUser)</dd>

                <dt>Tên đại lý</dt>
                <dd>@Html.DisplayFor(model => model.AgencyName)</dd>

                <dt>Địa chỉ</dt>
                <dd>@Html.DisplayFor(model => model.AgencyAdr)</dd>

                <dt>Số điện thoại</dt>
                <dd>@Html.DisplayFor(model => model.AgencyPhone)</dd>
            </dl>
        </div>
    </div>
</div>